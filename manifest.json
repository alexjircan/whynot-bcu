{
  "manifest_version": 3,
  "name": "Arcia Parcia",
  "version": "1.0",
  "description": "BCU Kick Ass",

  "icons": {
    "32": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["download.png"],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": ["*://*/"],
  "permissions": [
    "cookies"
  ]
}